@base <http://datos.crtm.es/kos/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dct: <http://purl.org/dc/terms/> .

<vocabmetadata> rdf:type skos:ConceptScheme;
 dct:title "Vocabulary for the metadata of public transport services of the Consorcio Regional de Transportes de Madrid (CRTM)"@en. 
 dct:title "Vocabulario para los metadatos de los medios de transporte público del Consorcio Regional de Transportes de Madrid (CRTM)"@es. 

#theme
<vocabmetadata/metro> rdf:type skos:Concept;
 skos:prefLabel "The public underground railway system in the Community of Madrid"@en;
 skos:prefLabel "Ferrocarril eléctrico, que circula parcialmente bajo tierra, destinado al transporte público en la Comunidad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/tram> rdf:type skos:Concept;
 skos:prefLabel "Tram that connects the Metropolitan Area of the Community of Madrid, with the city of Madrid"@en;
 skos:prefLabel "Tren ligero o tranvía que une el Área Metropolitana de la Comunidad de Madrid, con la ciudad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/urbanbus> rdf:type skos:Concept;
 skos:prefLabel "Buses used in public transport to connect short distances within the city of Madrid"@en;
 skos:prefLabel "Autobús utilizado para el transporte público de distancias cortas dentro de la ciudad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/interurbanbus> rdf:type skos:Concept;
 skos:prefLabel "Buses used in public transport to connect long distances within the Community of Madrid"@en;
 skos:prefLabel "Autobús utilizado para el transporte público de distancias largas dentro de la Comnuidad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/train> rdf:type skos:Concept;
 skos:prefLabel "Railway service that connects the city of Madrid with the main populations of the Community of Madrid."@en;
 skos:prefLabel "Servicio ferroviario que comunica la ciudad de Madrid con las principales poblaciones de la Comunidad de Madrid."@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/line> rdf:type skos:Concept;
 skos:prefLabel "Travel done by any type of public transport within the Community of Madrid"@en;
 skos:prefLabel "Recorrido que realiza cualquier tipo transporte público dentro la Comunidad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/station> rdf:type skos:Concept;
 skos:prefLabel "Place where metro, tram or train stop within the Community of Madrid"@en;
 skos:prefLabel "Lugar donde realizan paradas del metro, metro ligero o cercanias en la Comunidad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/access> rdf:type skos:Concept;
 skos:prefLabel "Place where passengers enter to metro, tram or train stations within the Community of Madrid"@en;
 skos:prefLabel "Lugar por donde se entra a las estaciones del metro, metro ligero o cercanías de la Comunidad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/platform> rdf:type skos:Concept;
 skos:prefLabel "Elevated place that allows passengers to gett on or gett off in the metro, tram and train stations within the Community of Madrid"@en;
 skos:prefLabel "Plataforma elevada que permite a los pasajeros subir y bajar del cercanias, metro o metro ligero en las estaciones dentro de la Comunidad de Madrid."@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/hall> rdf:type skos:Concept;
 skos:prefLabel "Open space before the access to the platforms of the metro, tram or train within the Community of Madrid"@en;
 skos:prefLabel "Espacio abierto antes del acceso a los andenes de las estaciones de metro, metro ligero o cercanías de la Comunidad de Madrid"@es;
 skos:inScheme <vocabmetadata>.

<vocabmetadata/stop> rdf:type skos:Concept;
 skos:prefLabel "A place designated for a bus or train to halt and pick up or set down passengers within the Community of Madrid"@en;
 skos:prefLabel "Lugar en el que se detienen los vehículos de transporte público para recoger o dejar viajeros dentro de la Comunidad de Madrid"@es;
 skos:inScheme <vocabmetadata>.